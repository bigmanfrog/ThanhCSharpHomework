<Window x:Class="Homework4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="uxZipCode" Height="25" TextWrapping="Wrap" VerticalAlignment="Top" Margin="59,41,616.6,0" TextChanged="UxZipCode_TextChanged"/>
        <Button x:Name="uxSubmit" Content="Submit" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="59,93,0,0" RenderTransformOrigin="-2.847,-8.848"/>
        <Label Content="Enter Zip Code:" HorizontalAlignment="Left" Margin="59,10,0,0" VerticalAlignment="Top" Height="26" Width="118"/>

    </Grid>
</Window>
